<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.trackyourpackage">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".RegisterPageRetailer"
            android:label="@string/title_activity_register_page_retailer"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".HomePageRetailer" />
        <activity android:name=".HomePageWarehouse" />
        <activity android:name=".HomePage" />
        <activity android:name=".RegisterPage" />
        <activity android:name=".LoginPageWarehouse" />
        <activity android:name=".LoginPageRetailer" />
        <activity android:name=".SelectLoginPage" />
        <activity android:name=".LoginPage" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>